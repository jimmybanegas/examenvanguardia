{
    "extends": "airbnb-base",
     "rules": {
         "no-console":0,
         "comma-dangle":"off",
         "class-methods-use-this":0,
         "brace-style":0,
         "no-restricted-syntax":0,
         "no-throw-literal":0,
         "no-unused-vars":0,
         "max-len":0,
         "no-useless-constructor":0,
         "no-empty-function":0
    },
    "env": {
        "jasmine": true
    }
}